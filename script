    - '@PLAYER &f[ icOS ] &cYou must kick everyone out first!'
    - '@EXIT'
    - '@ELSE'
    - '@SETSTR $admin1.<var:$channel.<playername>> null'
    - '@SETSTR $admin2.<var:$channel.<playername>> null'
    - '@SETSTR $admin3.<var:$channel.<playername>> null'
    - '@SETSTR $admin4.<var:$channel.<playername>> null'
    - '@SETSTR $admin5.<var:$channel.<playername>> null'
    - '@SETINT $channeladmincount.<var:$channel.<playername>> 0'
    - '@SETBOOL $channeladmin<playername>.<var:$channel.<playername>> false'
    - '@SETBOOL $haschannel.<playername> false'
    - '@SETBOOL $channelstorage.<var:$channel.<playername>> false'
    - '@SETBOOL $usingpassword.<var:$channel.<playername>> false'
    - '@SETSTR $channelowner.<var:$channel.<playername>> null'
    - '@PAUSE 1'
    - '@SETSTR $channel.<playername> null'
    - '@CALL g:leave'
    - '@PLAYER &f[ icOS ] &cChannel deleted.'
  adminmenu:
    Script:
    - '@IF i <cmdargcount> = 1'
    - '@OR i <cmdargcount> > 3'
    - '@OR si <cmdarg:2> = help'
    - '@PLAYER &a| &b/ge mute <player> &f- &6Mute <player> in your channel'
    - '@PLAYER &a| &b/ge unmute <player> &f- &6Unmute <player> in your channel'
    - '@PLAYER &a| &b/ge kick <player> &f- &6Kicks <player> from your channel'
    - '@PLAYER &a| &b/ge ban <player> &f- &6Bans <player> from your channel'
    - '@PLAYER &a| &b/ge unban <player> &f- &6Unbans <player> from your channel'
    - '@PLAYER &a| &b/ge filter on / off &f- &6Turns filter on / off'
    - '@PLAYER &a| &b/ge list &f- &6List all players in your channel'
    - '@PLAYER &b| &b/ge admin add <player> &f- &6Add someone to channel admin'
    - '@PLAYER &b| &b/ge admin remove <player> &f- &6Remove someone to channel admin'
    - '@PLAYER &e| &b/ge report <message> &f- &6Report a bug or suggestion to Hugs'
    - '@PLAYER &c| &b/ge ss &f- &6Social spy menu (staff only)'
    - '@EXIT'
    - '@ENDIF'
    - '@IF si <cmdarg:2> = remove'
    - ' @IF b $haschannel.<playername> = false'
    - ' @PLAYER &f[ icOS ] &cYou do not own any channels.'
    - ' @EXIT'
    - ' @ENDIF'
    - ' @IF i $channeladmincount.<var:$channel.<playername>> = 0'
    - ' @PLAYER &f[ icOS ] &cYou do not have any channel admins. I think you have had enough vodka tonight.'
    - ' @EXIT'
    - ' @ENDIF'
    - '@SUBINT $channeladmincount.<var:$channel.<playername>> 1'
    - '@IF s $admin1.<var:$channel.<playername>> = <cmdarg:3>'
    - '@SETSTR $admin1.<var:$channel.<playername>> null'
    - '@ENDIF'
    - '@IF s $admin2.<var:$channel.<playername>> = <cmdarg:3>'
    - '@SETSTR $admin2.<var:$channel.<playername>> null'
    - '@ENDIF'
    - '@IF s $admin3.<var:$channel.<playername>> = <cmdarg:3>'
    - '@SETSTR $admin3.<var:$channel.<playername>> null'
    - '@ENDIF'
    - '@IF s $admin4.<var:$channel.<playername>> = <cmdarg:3>'
    - '@SETSTR $admin4.<var:$channel.<playername>> null'
    - '@ENDIF'
    - '@IF s $admin5.<var:$channel.<playername>> = <cmdarg:3>'
    - '@SETSTR $admin5.<var:$channel.<playername>> null'
    - '@ENDIF'
    - '@SETSTR $channeladmin<cmdarg3>.<var:$channel.<playername>> false'
    - '@PLAYER &f[ icOS ] &bRemove &a<cmdarg:3> &bfrom channel admin!'
    - '@EXIT'
    - '@ENDIF'
    - '@IF si <cmdarg:2> = add'
    - '@IF b $haschannel.<playername> = false'
    - '@PLAYER &f[ icOS ] &cYou do not own any channels.'
    - '@EXIT'
    - '@ENDIF'
    - '@IF i $channeladmincount.<var:$channel.<playername>> = 5'
    - '@PLAYER &f[ icOS ] &cYou have 5/5 admins already. Considering that is half of the members allowed per channel, maybe you should stop while you are ahead...'
    - '@EXIT'
    - '@ENDIF'
    - '@SETBOOL $channeladmin<cmdarg3>.<var:$channel.<playername>> true'
    - '@ADDINT $channeladmincount.<var:$channel.<playername>> 1'
    - '@SETSTR $channelforcenick<cmdarg3>.<var:$channel.<playername>> &o'
    - '@PLAYER &f[ icOS ] &bAdded &a<cmdarg:3> &bto channel admin. Check /ge info for the list.'
    - '  @IF s $admin1.<var:$channel.<playername>> = null'
    - '  @SETSTR $admin1.<var:$channel.<playername>> <cmdarg:3>'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF s $admin2.<var:$channel.<playername>> = null'
    - '  @SETSTR $admin2.<var:$channel.<playername>> <cmdarg:3>'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF s $admin3.<var:$channel.<playername>> = null'
    - '  @SETSTR $admin3.<var:$channel.<playername>> <cmdarg:3>'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF s $admin4.<var:$channel.<playername>> = null'
    - '  @SETSTR $admin4.<var:$channel.<playername>> <cmdarg:3>'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF s $admin5.<var:$channel.<playername>> = null'
    - '  @SETSTR $admin5.<var:$channel.<playername>> <cmdarg:3>'
    - '  @EXIT'
    - '  @ENDIF'
    - '@ENDIF'
    - '@ENDIF'
  messageall:
    Script:
    - '@IF s $member1.<var:$activechannel.<playername>> != null'
    - '@TELL $member1.<var:$activechannel.<playername>> <var:$message.current>'
