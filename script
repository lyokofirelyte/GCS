Scripts:
  main:
    Script:
    - '@IF b $ge.globaldebug = true'
    - '@PLAYER &f[ icOS ] &cThe GCS is currently under maintence!'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 0'
    - '@OR si <cmdarg:1> = help'
    - '@CALL g:help'
    - '@ELSE'
    - '@IF si <cmdarg:1> = create'
    - '@CALL g:create'
    - '@ELSE'
    - '@IF si <cmdarg:1> = join'
    - '@CALL g:join'
    - '@ELSE'
    - '@IF si <cmdarg:1> = info'
    - '@CALL g:info'
    - '@ELSE'
    - '@IF si <cmdarg:1> = leave'
    - '@CALL g:leave'
    - '@ELSE'
    - '@IF si <cmdarg:1> = delete'
    - '@CALL g:delete'
    - '@ELSE'
    - '@IF si <cmdarg:1> = password'
    - '@CALL g:password'
    - '@ELSE'
    - '@IF si <cmdarg:1> = color'
    - '@CALL g:color'
    - '@ELSE'
    - '@IF si <cmdarg:1> = mute'
    - '@CALL g:mute'
    - '@ELSE'
    - '@IF si <cmdarg:1> = kick'
    - '@CALL g:kick'
    - '@ELSE'
    - '@IF si <cmdarg:1> = unmute'
    - '@CALL g:unmute'
    - '@ELSE'
    - '@IF si <cmdarg:1> = ban'
    - '@CALL g:ban'
    - '@ELSE'
    - '@IF si <cmdarg:1> = unban'
    - '@CALL g:unban'
    - '@ELSE'
    - '@IF si <cmdarg:1> = nick'
    - '@CALL g:nick'
    - '@ELSE'
    - '@IF si <cmdarg:1> = real'
    - '@CALL g:real'
    - '@ELSE'
    - '@IF si <cmdarg:1> = list'
    - '@CALL g:list'
    - '@ELSE'
    - '@IF si <cmdarg:1> = filter'
    - '@CALL g:filter'
    - '@ELSE'
    - '@IF si <cmdarg:1> = admin'
    - '@CALL g:adminmenu'
    - '@ELSE'
    - '@IF si <cmdarg:1> = ss'
    - '@CALL g:ss'
    - '@ELSE'
    - '@IF si <cmdarg:1> = report'
    - '@CMDOP mail send Hugh_Jasses &f[ icOS ] <cmdline>'
    - '@PLAYER &f[ icOS ] &bThank you for your bug report.'
    - '@ELSE'
    - '@CALL g:help'
    - '@ENDIF'
  real:
    Script:
    - '@IF i <cmdargcount> = 1'
    - '@PLAYER &f[ icOS ] &cUse /ge real <nick>'
    - '@EXIT'
    - '@ELSE'
    - '@IF b $existance.<cmdarg2> = false'
    - '@PLAYER &f[ icOS ] &cSupposed user &a<cmdarg:2> &cdoes not exist in the icOS database. Color and formatting codes must be included in the lookup sequence.'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 2'
    - '@PLAYER &f[ icOS ] &a<cmdarg2> &bis actually &a<var:$channelreal.<cmdarg2>>&b.'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cUse /ge real <nick>'
    - '@ENDIF'
  nick:
    Script:
    - '@IF i <cmdargcount> = 1'
    - '@PLAYER &f[ icOS ] &cUse /ge nick <nick>'
    - '@EXIT'
    - '@ELSE'
    - '@IF si <cmdarg:2> = clear'
    - '@SETBOOL $usingnick.<playername> false'
    - '@SETBOOL $existance.<var:$channelnick.<playername>> false'
    - '@SETSTR $channelnick.<playername> null'
    - '@PLAYER &f[ icOS ] &bNickname reset to &a<playername>&b.'
    - '@EXIT'
    - '@ELSE'
    - '@IF b $existance.<cmdarg:2> = true'
    - '@PLAYER &f[ icOS ] &cSomeone already has that nickname! Not naming any names, &a&o<var:$channelreal.<cmdarg2>>&c.'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 2'
    - '@CALL gcensor:main'
    - '@IF b $stopchat.<playername> = true'
    - '@SETBOOL $stopchat.<playername> false'
    - '@PLAYER &f[ icOS ] &cVulgar nicknames are not allowed. Please do not try to avoid this.'
    - '@EXIT'
    - '@ELSE'
    - '@SETBOOL $existance.<var:$channelnick.<playername>> false'
    - '@SETBOOL $existance.<cmdarg:2> true'
    - '@SETBOOL $usingnick.<playername> true'
    - '@SETSTR $channelnick.<playername> <cmdarg:2>'
    - '@SETSTR $channelreal.<cmdarg2> <playername>'
    - '@PLAYER &f[ icOS ] &bYour nickname is now &a<cmdarg2>&b.'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cUse /ge nick <nick>'
    - '@ENDIF'
  ss:
    Script:
    - '@IF b <haspermission:ic.staff> = false'
    - '@PLAYER &f[ icOS ] You are not authorized to use this command.'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 1'
    - '@OR si <cmdarg:2> = help'
    - '@PLAYER &e| &b/ge ss on &f- &6Turn on social spy'
    - '@PLAYER &e| &b/ge ss off &f- &6Turn off social spy'
    - '@PLAYER &c| &b/ge ss add <player> &f- &6Add a player to the SS list (owner only)'
    - '@PLAYER &4| &b/ge ss clr &f- &6Clear the SS list (owner only)'
    - '@EXIT'
    - '@ELSE'
    - '@IF si <cmdarg:2> = on'
    - '  @IF b $ssq.<playername> = false'
    - '  @PLAYER &f[ icOS ] &cYou were not added to the SS list.'
    - '  @EXIT'
    - '  @ENDIF'
    - '@SETBOOL $ss.<playername> true'
    - '@PLAYER &f[ icOS ] &bSocial spy enabled!'
    - '@EXIT'
    - '@ELSE'
    - '@IF si <cmdarg:2> = off'
    - '  @IF b $ssq.<playername> = false'
    - '  @PLAYER &f[ icOS ] &cYou were not added to the SS list.'
    - '  @EXIT'
    - '  @ENDIF'
    - '@SETBOOL $ss.<playername> false'
    - '@PLAYER &f[ icOS ] &bSocial spy disabled!'
    - '@EXIT'
    - '@ELSE'
    - '@IF si <cmdarg:2> = clr'
    - '@CALL g:ssc'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 3'
    - '@CALL g:ssa'
    - '@EXIT'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cType /ge ss help for help.'
    - '@ENDIF'
  filter:
    Script:
    - '@IF b $haschannel.<playername> = false'
    - '@PLAYER &f[ icOS ] &cYou need a channel in order to apply a filter!'
    - '@EXIT'
    - '@ELSE'
    - '@IF si <cmdarg:2> = on'
    - '@SETBOOL $usingfilter.<var:$channel.<playername>> true'
    - '@PLAYER &f[ icOS ] &bFilter &aenabled&b.'
    - '@EXIT'
    - '@ELSE'
    - '@IF si <cmdarg:2> = off'
    - '@SETBOOL $usingfilter.<var:$channel.<playername>> false'
    - '@PLAYER &f[ icOS ] &bFilter &4disabled&b.'
    - '@EXIT'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cUse /ge filter on or /ge filter off'
    - '@ENDIF'
  unban:
    Script:
    - '@IF b $channeladmin<playername>.<var:$activechannel.<playername>> = false'
    - '@PLAYER &f[ icOS ] &cYou are not a channel admin.'
    - '@EXIT'
    - '@ENDIF'
    - '@IF b $joinedchannel.<playername> = false'
    - '@PLAYER &f[ icOS ] &cYou are not in a channel.'
    - '@EXIT'
    - '@ENDIF'
    - '@IF i <cmdargcount> = 1'
    - '@OR i <cmdargcount> > 2'
    - '@PLAYER &f[ icOS ] &cUse /ge unban <player>'
    - '@EXIT'
    - '@ENDIF'
    - '@SETBOOL $banned<cmdarg2>.<var:$activechannel.<playername>> false'
    - '@SETSTR $message.current &b<cmdarg:2> &ewas unbanned from the channel by <playername>.'
    - '@PAUSE 0.5'
    - '@CALL g:messageall'
  ban:
    Script:
    - '@IF b $channeladmin<playername>.<var:$activechannel.<playername>> = false'
    - '@PLAYER &f[ icOS ] &cYou are not a channel admin.'
    - '@EXIT'
    - '@ENDIF'
    - '@IF b $joinedchannel.<playername> = false'
    - '@PLAYER &f[ icOS ] &cYou are not in a channel.'
    - '@EXIT'
    - '@ENDIF'
    - '@IF i <cmdargcount> = 1'
    - '@OR i <cmdargcount> > 2'
    - '@PLAYER &f[ icOS ] &cUse /ge ban <player>'
    - '@EXIT'
    - '@ENDIF'
    - '@IF s $channelowner.<var:$activechannel.<playername>> = <cmdarg:2>'
    - '@PLAYER &f[ icOS ] &cYou can not ban the owner of this channel!'
    - '@EXIT'
    - '@ENDIF'
    - '@SETBOOL $banned<cmdarg2>.<var:$activechannel.<playername>> true'
    - '@SETSTR $message.current &b<cmdarg:2> &ewas banned from the channel by <playername>. Kicking...'
    - '@PAUSE 0.5'
    - '@CALL g:messageall'
    - '@CALL g:kick'
  kick:
    Script:
    - '@IF b $channeladmin<playername>.<var:$activechannel.<playername>> = false'
    - '@PLAYER &f[ icOS ] &cYou are not a channel admin.'
    - '@EXIT'
    - '@ENDIF'
    - '@IF b $joinedchannel.<playername> = false'
    - '@PLAYER &f[ icOS ] &cYou are not in a channel.'
    - '@EXIT'
    - '@ENDIF'
    - '@IF s $channelowner.<var:$activechannel.<playername>> = <cmdarg:2>'
    - '@PLAYER &f[ icOS ] &cYou can not kick the owner of this channel!'
    - '@EXIT'
    - '@ENDIF'
    - '@IF i <cmdargcount> = 1'
    - '@OR i <cmdargcount> > 2'
    - '@PLAYER &f[ icOS ] &cUse /ge kick <player>'
    - '@EXIT'
    - '@ENDIF'
    - '@IF i <cmdargcount> = 2'
    - '@SETBOOL $joinedchannel.<cmdarg:2> false'
    - '@SETSTR $message.current &b<cmdarg:2> &ewas kicked from the channel by <playername>.'
    - '@PAUSE 0.5'
    - '@CALL g:messageall'
    - '@SUBINT $channelmembers.<var:$activechannel.<playername>> 1'
    - '@IF s $member1.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member1.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member2.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member2.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member3.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member3.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member4.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member4.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member5.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member5.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member6.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member6.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member7.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member7.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member8.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member8.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member9.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member9.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@IF s $member10.<var:$activechannel.<playername>> = <cmdarg2>'
    - '@SETSTR $member10.<var:$activechannel.<playername>> null'
    - '@ENDIF'
    - '@SETSTR $activechannel.<cmdarg:2> null'
  list:
    Script:
    - '@IF b $joinedchannel.<playername> = false'
    - '@PLAYER &f[ icOS ] &cYou are not in a channel!'
    - '@EXIT'
    - '@ENDIF'
    - '@PLAYER &b[ <var:$activechannel.<playername>> ]'
    - '@IF s $member1.<var:$activechannel.<playername>> != null'
    - '@AND s $member1.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member1.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member2.<var:$activechannel.<playername>> != null'
    - '@AND s $member2.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member2.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member3.<var:$activechannel.<playername>> != null'
    - '@AND s $member3.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member3.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member4.<var:$activechannel.<playername>> != null'
    - '@AND s $member4.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member4.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member5.<var:$activechannel.<playername>> != null'
    - '@AND s $member5.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member5.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member6.<var:$activechannel.<playername>> != null'
    - '@AND s $member6.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member6.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member7.<var:$activechannel.<playername>> != null'
    - '@AND s $member7.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member7.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member8.<var:$activechannel.<playername>> != null'
    - '@AND s $member8.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member8.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member9.<var:$activechannel.<playername>> != null'
    - '@AND s $member9.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member9.<var:$activechannel.<playername>>'
    - '@ENDIF'
    - '@IF s $member10.<var:$activechannel.<playername>> != null'
    - '@AND s $member10.<var:$activechannel.<playername>> != 0'
    - '@PLAYER $member10.<var:$activechannel.<playername>>'
    - '@ENDIF'
  password:
    Script:
    - '@IF b $haschannel.<playername> = false'
    - '@PLAYER &f[ icOS ] &cYou do not have a channel to add a password to!'
    - '@EXIT'
    - '@ELSE'
    - '@IF si <cmdarg:2> = clear'
    - '@SETBOOL $usingpassword.<var:$channel.<playername>> false'
    - '@SETSTR $password.<var:$channel.<playername>> null'
    - '@PLAYER &f[ icOS ] &bPassword removed!'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 2'
    - '@SETBOOL $usingpassword.<var:$channel.<playername>> true'
    - '@SETSTR $password.<var:$channel.<playername>> <cmdarg:2>'
    - '@PLAYER &f[ icOS ] &bPassword changed to &a<cmdarg:2>&b.'
    - '@EXIT'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cUse /ge password <password> or /ge password clear'
    - '@ENDIF'
  mute:
    Script:
    - '@IF b $channeladmin<playername>.<var:$activechannel.<playername>> = false'
    - '@PLAYER &f[ icOS ] &cYou are not a channel admin.'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 2'
    - '@SETBOOL $muted<cmdarg2>.<var:$activechannel.<playername>> true'
    - '@SETSTR $message.current &b<cmdarg:2> &ehas been muted in the channel by <playername>.'
    - '@PAUSE 0.5'
    - '@CALL g:messageall'
    - '@EXIT'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cUse /ge mute <player>'
    - '@ENDIF'
  unmute:
    Script:
    - '@IF b $channeladmin<playername>.<var:$activechannel.<playername>> = false'
    - '@PLAYER &f[ icOS ] &cYou are not a channel admin.'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 2'
    - '@SETBOOL $muted<cmdarg2>.<var:$activechannel.<playername>> false'
    - '@SETSTR $message.current &b<cmdarg:2> &ehas been unmuted in the channel by <playername>.'
    - '@PAUSE 0.5'
    - '@CALL g:messageall'
    - '@EXIT'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cUse /ge unmute <player>'
    - '@ENDIF'
  help:
    Script:
    - '@PLAYER &f[ icOS ] &bImagicraft Group Chat System v1.2'
    - '@PLAYER &f--- --- ---'
    - '@PLAYER &a| &b/ge create <channel> [optional password] &f- &6Names can be one word only'
    - '@PLAYER &a| &b/ge join <channel> [password] &f- &6Joins a channel'
    - '@PLAYER &a| &b/ge leave &f- &6Leaves current channel'
    - '@PLAYER &a| &b/ge delete &f- &6Deletes the channel owned by you'
    - '@PLAYER &a| &b/ge info <channel> &f- &6Displays <channel> info'
    - '@PLAYER &a| &b/ge password <password> &f- &6Set a password for your channel'
    - '@PLAYER &a| &b/ge nick <nick> &f- &6Set a channel nickname for yourself'
    - '@PLAYER &a| &b/ge real <nick> &f- &6Find the person who has <nick>'
    - '@PLAYER &a| &b/ge color list &f- &6List all color codes that are usable'
    - '@PLAYER &1| &b/ge admin &f- &6Display admin commands for your channel'
    - '@PLAYER &b| &b/p <message> &f- &6Talk in current channel'
    - '@PLAYER &f--- --- ---'
    - '@PLAYER &aGCS for icOS coded from scratch by Hugs (v1.2 stable)'
  color:
   Script:
    - '@IF si <cmdarg:2> = list'
    - '@PLAYER &f[ icOS ] &bColor Code List'
    - '@PLAYER &a| a = light green'
    - '@PLAYER &b| b = light blue'
    - '@PLAYER &c| c = salmon'
    - '@PLAYER &d| d = purple'
    - '@PLAYER &e| e = yellow'
    - '@PLAYER &f| f = white'
    - '@PLAYER &0| 0 = black'
    - '@PLAYER &1| 1 = dark blue'
    - '@PLAYER &2| 2 = dark green'
    - '@PLAYER &3| 3 = teal'
    - '@PLAYER &4| 4 = dark red'
    - '@PLAYER &5| 5 = dark purple'
    - '@PLAYER &6| 6 = gold'
    - '@PLAYER &7| 7 = light grey'
    - '@PLAYER &8| 8 = dark grey'
    - '@PLAYER &9| 9 = blue'
    - '@EXIT'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &bFull list of codes under /ge color list.'
    - '@ENDIF'
  create:
    Script:
    - '@IF i <cmdargcount> = 1'
    - '@PLAYER &f[ icOS ] &cUse /ge create <channel> [optional password]'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 2'
    - '  @IF b $haschannel.<playername> = true'
    - '  @PLAYER &f[ icOS ] &cYou already have an active channel. Use /ge delete to remove it.'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF b $channelstorage.<cmdarg:2> = true'
    - '  @PLAYER &f[ icOS ] &cThat channel already exists!'
    - '  @EXIT'
    - '  @ENDIF'
    - '@SETBOOL $haschannel.<playername> true'
    - '@SETSTR $channelowner.<cmdarg:2> <playername>'
    - '@SETSTR $channel.<playername> <cmdarg:2>'
    - '@SETBOOL $channelstorage.<cmdarg:2> true'
    - '@SETBOOL $channeladmin<playername>.<var:$channel.<playername>> true'
    - '@SETSTR $admin1.<var:$channel.<playername>> null'
    - '@SETSTR $admin2.<var:$channel.<playername>> null'
    - '@SETSTR $admin3.<var:$channel.<playername>> null'
    - '@SETSTR $admin4.<var:$channel.<playername>> null'
    - '@SETSTR $admin5.<var:$channel.<playername>> null'
    - '@SETINT $channeladmincount.<var:$channel.<playername>> 0'
    - '@SETSTR $member1.<var:$channel.<playername>> null'
    - '@SETSTR $member2.<var:$channel.<playername>> null'
    - '@SETSTR $member3.<var:$channel.<playername>> null'
    - '@SETSTR $member4.<var:$channel.<playername>> null'
    - '@SETSTR $member5.<var:$channel.<playername>> null'
    - '@SETSTR $member6.<var:$channel.<playername>> null'
    - '@SETSTR $member7.<var:$channel.<playername>> null'
    - '@SETSTR $member8.<var:$channel.<playername>> null'
    - '@SETSTR $member9.<var:$channel.<playername>> null'
    - '@SETSTR $member10.<var:$channel.<playername>> null'
    - '@PLAYER &f[ icOS ] &bCreated channel &a<cmdarg:2> &bwith default options. See /ge help for more options. Join this channel with &f/ge join <cmdarg:2>&b.'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 3'
    - '  @IF b $haschannel.<playername> = true'
    - '  @PLAYER &f[ icOS ] &cYou already have an active channel. Use /ge delete to remove it.'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF b $channelstorage.<cmdarg:2> = true'
    - '  @PLAYER &f[ icOS ] &cThat channel already exists!'
    - '  @EXIT'
    - '  @ENDIF'
    - '@SETBOOL $haschannel.<playername> true'
    - '@SETSTR $channelowner.<cmdarg:2> <playername>'
    - '@SETSTR $channel.<playername> <cmdarg:2>'
    - '@SETBOOL $channelstorage.<cmdarg:2> true'
    - '@SETBOOL $usingpassword.<cmdarg:2> true'
    - '@SETSTR $password.<cmdarg:2> <cmdarg:3>'
    - '@SETBOOL $channeladmin<playername>.<var:$channel.<playername>> true'
    - '@SETSTR $member1.<var:$channel.<playername>> null'
    - '@SETSTR $member2.<var:$channel.<playername>> null'
    - '@SETSTR $member3.<var:$channel.<playername>> null'
    - '@SETSTR $member4.<var:$channel.<playername>> null'
    - '@SETSTR $member5.<var:$channel.<playername>> null'
    - '@SETSTR $member6.<var:$channel.<playername>> null'
    - '@SETSTR $member7.<var:$channel.<playername>> null'
    - '@SETSTR $member8.<var:$channel.<playername>> null'
    - '@SETSTR $member9.<var:$channel.<playername>> null'
    - '@SETSTR $member10.<var:$channel.<playername>> null'
    - '@SETINT $channelmembers.<var:$channel.<playername> 0'
    - '@PLAYER &f[ icOS ] &bCreated channel &a<cmdarg:2> &bwith password &a<cmdarg:3>&b. See /ge help for more options. Join this channel with &f/ge join <cmdarg:2> <cmdarg:3>&b.'
    - '@EXIT'
    - '@ELSE'
    - '@PLAYER &f[ icOS ] &cUse /ge create <channel> [optional password]'
    - '@ENDIF'
  join:
    Script:
    - '@IF b $joinedchannel.<playername> = true'
    - '@PLAYER &f[ icOS ] &cYou are already in a channel. Type /ge leave first.'
    - '@EXIT'
    - '@ELSE'
    - '@IF i <cmdargcount> = 1'
    - '@PLAYER &f[ icOS ] &cUse /ge join <channel> [optional password]'
    - '@EXIT'
    - '@ELSE'
    - '@IF b $usingpassword.<cmdarg:2> = true'
    - '@SETSTR $vartransfer.<playername> <cmdarg:2>'
    - '@PAUSE 0.3'
    - '  @IF si $password.<var:$vartransfer.<playername>> != <cmdarg:3>'
    - '  @PLAYER &f[ icOS ] &cInvalid password.'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF i $channelmembers.<var:$vartransfer.<playername>> = 10'
    - '  @PLAYER &f[ icOS ] &cThat channel is currently full.'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF b $banned<playername>.<var:$vartransfer.<playername>> = true'
    - '  @PLAYER &f[ icOS ] &cYou were banned from that channel.'
    - '  @EXIT'
    - '  @ENDIF'
    - '  @IF b $channelstorage.<cmdarg:2> = false'
    - '  @PLAYER &f[ icOS ] &cThat channel does not exist!'
    - '  @EXIT'
    - '  @ENDIF'
    - '@SETSTR $channelowner.info<playername> <var:$channelowner.<cmdarg2>'
    - '@SETBOOL $joinedchannel.<playername> true'
    - '@SETSTR $activechannel.<playername> <cmdarg:2>'
    - '@ADDINT $channelmembers.<var:$vartransfer.<playername>> 1'
    - '@SETSTR $message.current &b<playername> &ehas joined the channel.'
